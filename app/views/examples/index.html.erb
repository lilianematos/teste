<div class="page-header">
<h1 class="titulo">Artigos Privados</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Pasta</th>
      
    </tr>
  </thead>

  <tbody>
    <% @example_privates.each do |example| %>

      <% @folder = Folder.where(user_id: current_user.id) %>

        <% @folder.each do |fold| %>

          <% if fold.id==example.folder_id %>

            <div class="link_too">    
                <tr>
                  <td class="td-index"><%= link_to example.name, example %></td>
                  <td><%= example.folder.name %></td>
                  
                </tr>
        
            </div>

          <% end %>

        <% end %>

    <% end %>
    <br>
  </tbody>
</table>

<br>

<%= will_paginate @example_privates, renderer: BootstrapPagination::Rails %>

</div>

</div>